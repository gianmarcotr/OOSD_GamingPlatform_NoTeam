    <servlet>
        <servlet-name>UserDAO</servlet-name>
        <servlet-class>org.oosd.project.utils.DAO.UserDAO</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>org.oosd.project.servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>profileServlet</servlet-name>
        <servlet-class>org.oosd.project.servlet.ProfileServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GameServlet</servlet-name>
        <servlet-class>org.oosd.project.servlet.GameServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReviewServlet</servlet-name>
        <servlet-class>org.oosd.project.servlet.ReviewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>gameSessionServlet</servlet-name>
        <servlet-class>org.oosd.project.servlet.GameSessionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>playServlet</servlet-name>
