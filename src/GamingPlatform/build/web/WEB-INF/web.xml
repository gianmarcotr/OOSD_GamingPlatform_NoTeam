        <servlet-class>org.oosd.project.servlet.SignupServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SignupOwnerServlet</servlet-name>
        <servlet-class>org.oosd.project.servlet.SignupOwnerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProfileOwnerServlet</servlet-name>
        <servlet-class>org.oosd.project.servlet.ProfileOwnerServlet</servlet-class>
