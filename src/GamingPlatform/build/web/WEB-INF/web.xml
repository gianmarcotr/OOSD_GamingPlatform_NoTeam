    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewAchiServlet</servlet-name>
        <url-pattern>/NewAchiServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GameOwnerServlet</servlet-name>
        <url-pattern>/GameOwnerServlet</url-pattern>
    </servlet-mapping>
